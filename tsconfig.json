{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2024",
    "lib": ["es2024", "dom"],
    "types": ["node"],
    "skipLibCheck": true,
    "isolatedModules": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  }
}
