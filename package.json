{
  "name": "@seia-soto/checkadb",
  "version": "1.1.0",
  "description": "Half-automated ad-blocking capability end-to-end testing framework.",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && cp -r ./src/server/www ./dist/server",
    "www": "node --import=tsimp/import ./scripts/www.ts",
    "lint": "eslint .",
    "style": "prettier .",
    "check": "pnpm lint && pnpm style",
    "fix": "pnpm lint --fix && pnpm style --write"
  },
  "keywords": [],
  "author": "HoJeong Go <seia@outlook.kr>",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.5.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsimp": "^2.0.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0"
  }
}
